<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane fx:controller="net.askello.requesttester.controller.control.FilesController" minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="597.0" style="-fx-background-color: #cecece;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <TableView fx:id="filesTable" layoutX="14.0" layoutY="3.0" prefHeight="150.0" prefWidth="285.0">
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
         <columns>
            <TableColumn fx:id="keyFileColumn" prefWidth="75.0" text="key" />
            <TableColumn fx:id="pathFileColumn" prefWidth="75.0" text="path" />
         </columns>
      </TableView>
      <Button fx:id="removeFileButton" layoutX="314.0" layoutY="75.0" mnemonicParsing="false" onAction="#removeFileButtonHandler" prefHeight="25.0" prefWidth="84.0" text="remove" />
      <Button fx:id="editFileButton" layoutX="314.0" layoutY="39.0" mnemonicParsing="false" onAction="#editFileButtonHandler" prefHeight="25.0" prefWidth="84.0" text="edit" />
      <Button fx:id="addFileButton" layoutX="314.0" layoutY="3.0" mnemonicParsing="false" onAction="#addFileButtonHandler" prefHeight="25.0" prefWidth="84.0" text="add" />
   </children>
</AnchorPane>
